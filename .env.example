# DBDash Configuration
# Copy this file to .env and configure your databases

# Authentication
PASSWORD=your_secure_password_here
JWT_SECRET=your_jwt_secret_here

# Ports
FRONTEND_PORT=8888
PORT=8889

# Database Configuration
# Format: DB_X_TYPE, DB_X_ID, DB_X_NAME, DB_X_HOST, DB_X_PORT, DB_X_USER, DB_X_PASSWORD, DB_X_DATABASE

# Example: PostgreSQL Database
DB_1_TYPE=postgres
DB_1_ID=my_postgres_db
DB_1_NAME=My PostgreSQL Database
DB_1_HOST=localhost
DB_1_PORT=5432
DB_1_USER=postgres
DB_1_PASSWORD=your_password_here
DB_1_DATABASE=mydb

# Example: MySQL Database
DB_2_TYPE=mysql
DB_2_ID=my_mysql_db
DB_2_NAME=My MySQL Database
DB_2_HOST=localhost
DB_2_PORT=3306
DB_2_USER=root
DB_2_PASSWORD=your_password_here
DB_2_DATABASE=mydb
